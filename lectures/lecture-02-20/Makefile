########################################################################
#
# A simple Makefile for compiling Java code for CS392X1
#
########################################################################
JAVA=java
JAVAC=javac
########################################################################
MYCPS=.:./../../assets/algs4/algs4.jar
########################################################################
all::
all:: \
Stack
########################################################################
Stack: Stack.class; $(JAVA) -cp $(MYCPS) Stack < tobe.txt
Stack.class: Stack.java; $(JAVAC) -cp $(MYCPS) Stack.java
########################################################################
clean:: ; rm -f *~
########################################################################
cleanall:: clean
cleanall:: ; rm -f *.class
########################################################################
########################################################################
########################################################################
